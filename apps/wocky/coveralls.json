{
  "default_stop_words": [
    "defmodule",
    "defrecord",
    "defimpl",
    "def.+(.+\/\/.+).+do"
  ],

  "custom_stop_words": [
    "defenum",
    "defstruct",
    "schema.+\".+\".+do"
  ],

  "skip_files": [
    "spec",
    "test",
    "deps"
  ],

  "coverage_options": {
    "treat_no_relevant_lines_as_covered": true,
    "output_dir": "cover/",
    "minimum_coverage": 80
  }
}
